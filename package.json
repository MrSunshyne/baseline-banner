{
  "name": "@baseline-banner/monorepo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "type-check": "pnpm -r type-check",
    "lint": "pnpm -r lint",
    "test": "vitest run",
    "test:packages": "pnpm -r test",
    "test:watch": "vitest",
    "generate-types": "cd packages/core && node scripts/generate-feature-types.js",
    "pack:local": "pnpm -r --filter=!playground build && cd packages/core && npm pack && cd ../vue && cp package.json package.json.bak && sed 's/\"workspace:\\*\"/\"0.1.0\"/' package.json > package.json.tmp && mv package.json.tmp package.json && npm pack && mv package.json.bak package.json",
    "pack:clean": "rm -f packages/core/*.tgz packages/vue/*.tgz",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^22.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "react": "^18.3.1",
    "react-dom": "^18",
    "typescript": "^5.6.3",
    "vite": "^6.0.0",
    "vitest": "^2.1.0",
    "vue": "^3.5.21"
  }
}
