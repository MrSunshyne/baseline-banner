{
  "name": "@baseline-banner/monorepo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "type-check": "pnpm -r type-check",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "generate-types": "cd packages/core && node scripts/generate-feature-types.js",
    "pack:local": "pnpm build && cd packages/core && npm pack && cd ../vue && cp package.json package.json.bak && sed 's/\"workspace:\\*\"/\"0.1.0\"/' package.json > package.json.tmp && mv package.json.tmp package.json && npm pack && mv package.json.bak package.json",
    "pack:clean": "rm -f packages/core/*.tgz packages/vue/*.tgz",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@types/node": "^22.3.0",
    "typescript": "^5.6.3",
    "vite": "^6.0.0",
    "vitest": "^2.1.0"
  }
}
